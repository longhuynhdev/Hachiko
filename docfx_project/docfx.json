{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Hachiko/**.csproj",
            "Hachiko.DataAccess/**.csproj",
            "Hachiko.Models/**.csproj",
            "Hachiko.Utility/**.csproj"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "SamePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Hachiko E-Commerce Project",
      "_appFooter": "Â© 2023 - CLC (20HTTT) | Hachiko E-Commerce Documentation",
      "_enableSearch": true,
      "_disableBreadcrumb": false,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableTocFilter": false,
      "_disableAffix": false
    },
    "fileMetadata": {},
    "cleanupCacheHistory": false,
    "disableParallelProcessing": false
  }
}
